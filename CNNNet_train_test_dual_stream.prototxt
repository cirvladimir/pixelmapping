  top: "conv4_0"
  param {
    name: "param_10"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "param_11"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "relu4_0"
  type: "PReLU"
  bottom: "conv4_0"
  top: "conv4_0"
  param {
    name: "param_12"
    lr_mult: 1
  }
