    name: "param_8"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "relu3_0"
  type: "PReLU"
  bottom: "conv3_0"
  top: "conv3_0"
  param {
    name: "param_9"
    lr_mult: 1
  }
