  }
  param {
    name: "param_11"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1
