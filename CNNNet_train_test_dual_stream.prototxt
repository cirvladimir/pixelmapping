# Full spec of training an RGB network.
name: "RGBNet"
# Image Data Layers
input: "image_data"
input_dim: 48
input_dim: 2
input_dim: 100
input_dim: 100

input: "raw_label_pose"
input_dim: 48
input_dim: 121
input_dim: 1
input_dim: 1


layer {
  name: "image_data_slice"
  type: "Slice"
  bottom: "image_data"
  top: "image_data_0"
  top: "image_data_1"
  slice_param {
    slice_dim: 1
    slice_point: 1
  }
}

# Stream 0
layer {
  name: "conv1_0"
  type: "Convolution"
  bottom: "image_data_0"
  top: "conv1_0"
  param {
    name: "param_1"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "param_2"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 100
    kernel_size: 3
    pad: 1
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_0"
  type: "PReLU"
  bottom: "conv1_0"
  top: "conv1_0"
  param {
    name: "param_3"
    lr_mult: 1
  }
  prelu_param {
    filler {
      type: "constant"
      value: 0.25
    }
  }
}
layer {
  name: "pool1_0"
  type: "Pooling"
  bottom: "conv1_0"
  top: "pool1_0"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "conv2_0"
  type: "Convolution"
  bottom: "pool1_0"
  top: "conv2_0"
  param {
    name: "param_4"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "param_5"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "relu2_0"
  type: "PReLU"
  bottom: "conv2_0"
  top: "conv2_0"
