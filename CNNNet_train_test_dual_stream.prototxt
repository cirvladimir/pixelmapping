    }
  }
}
layer {
  name: "pool4_0"
  type: "Pooling"
  bottom: "conv4_0"
  top: "pool4_0"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "conv5_0"
  type: "Convolution"
  bottom: "pool4_0"
  top: "conv5_0"
  param {
    name: "param_13"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "param_14"
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "relu5_0"
  type: "PReLU"
  bottom: "conv5_0"
  top: "conv5_0"
  param {
    name: "param_15"
    lr_mult: 1
  }
  prelu_param {
    filler {
      type: "constant"
      value: 0.25
    }
  }
}
layer {
  name: "pool5_0"
  type: "Pooling"
  bottom: "conv5_0"
  top: "pool5_0"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "ip_0"
  type: "InnerProduct"
  bottom: "pool5_0"
  top: "ip_0"
  param {
    name: "param_16"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "param_17"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 2048
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1.0
    }
  }
}
layer {
  name: "relu_0"
  type: "PReLU"
  bottom: "ip_0"
  top: "ip_0"
  param {
    name: "param_18"
    lr_mult: 1
  }
  prelu_param {
    filler {
      type: "constant"
      value: 0.25
    }
  }
}





