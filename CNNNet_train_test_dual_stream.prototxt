# FC Layers
layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "stream_concat"
  top: "fc6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 2048
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1.0
    }
  }
}
layer {
  name: "relu6"
  type: "PReLU"
  bottom: "fc6"
  top: "fc6"
  param {
    lr_mult: 1
  }
  prelu_param {
    filler {
      type: "constant"
      value: 0.25
    }
  }
}
layer {
  name: "drop6"
  type: "Dropout"
  bottom: "fc6"
  top: "fc6"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "fc7"
  type: "InnerProduct"
  bottom: "fc6"
  top: "fc7"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1.0
    }
  }
}
layer {
  name: "drop7"
  type: "Dropout"
  bottom: "fc7"
  top: "fc7"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "relu_fc7"
  type: "PReLU"
  bottom: "fc7"
  top: "fc7"
  param {
    lr_mult: 1
  }
  prelu_param {
    filler {
      type: "constant"
      value: 0.25
    }
  }
}

layer {
  name: "fc8"
  type: "InnerProduct"
  bottom: "fc7"
  top: "fc8"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 121
    weight_filler {
      type: "prelu"
    }
    bias_filler {
      type: "constant"
      value: 1.0
    }
  }
}
layer {
  name: "softmax"
  type: "Softmax"
  bottom: "fc8"
  top: "softmax"
}
layer {
  name: "regression_loss"
  type: "SmoothL1Loss"
  bottom: "softmax"
  bottom: "raw_label_pose"
  top: "regression_loss"
  loss_weight: 1
}